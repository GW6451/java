package accessmodifier07;
/*
 * **protected접근지정자  :
 * 		같은 패키지는 물론 다른 패키지 일지라도 두 클래스 간의 상속관계가 존재한다면 접근 가능
 */
public class Super {
	//멤버 메소드]
	private void privateMehtod() {}
	void packageMehthod() {}
	protected void protectedMethod() {}
	public void publicMethod() {}
	
	void callAll() {
		privateMehtod();
		packageMehthod();
		protectedMethod();
		publicMethod();
	}

}
